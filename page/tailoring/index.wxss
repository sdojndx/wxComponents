/* page/tailoring/index.wxss */
.image_list{
    display: flex;
    flex-wrap:wrap;
    justify-content: center;
}
.image_list image{
    margin-top:15rpx;
    width:220rpx;
    height: 220rpx;
}
.upload_bt{
    width:300rpx;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 8rpx;
    display: block;
    margin: 20rpx auto;
    text-indent: 20rpx;
    color:black;
    background:#ccc;
    text-align: center;
}
.batch_tailoring_s{
    position: fixed;
    top: 0;
    left:0;
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    background:rgba(0, 0, 0, 0.3);
}
.batch_tailoring{
    flex: 1;
}